{
  "version": 1,
  "dfx": "0.14.1",
  "canisters": {
    "zk_canister": {
      "type": "rust",
      "package": "zk_canister",
      "candid": "src/zk_canister/zk_canister.did"
    },
    "main_canister": {
      "type": "rust",
      "package": "main_canister",
      "candid": "src/main_canister/main_canister.did"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "package_set": [
    {
      "name": "base",
      "repo": "https://github.com/dfinity/motoko-base",
      "version": "moc-0.9.1",
      "dependencies": []
    }
  ]
}