{
  "canisters": {
    "zk_canister": {
      "candid": "backend/zk_canister/zk_canister.did",
      "package": "zk_canister",
      "type": "rust",
      "source": ["backend/zk_canister/src"]
    },
    "main_canister": {
      "candid": "backend/main_canister/main_canister.did",
      "package": "main_canister",
      "type": "rust",
      "source": ["backend/main_canister/src"]
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "version": 1,
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  }
} 